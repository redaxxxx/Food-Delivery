<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.prof.reda.android.project.fooddelivery.views.fragments.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_popularRestroFragment"
            app:destination="@id/popularRestroFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_homeFragment_to_popularMenuFragment"
            app:destination="@id/popularMenuFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_homeFragment_to_filterFragment"
            app:destination="@id/filterFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.prof.reda.android.project.fooddelivery.views.fragments.home.ChatFragment"
        android:label="ChatFragment"
        tools:layout="@layout/fragment_chat">
        <action
            android:id="@+id/action_chatFragment2_to_chatDetailsFragment"
            app:destination="@id/chatDetailsFragment" />

    </fragment>
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.prof.reda.android.project.fooddelivery.views.fragments.home.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.prof.reda.android.project.fooddelivery.views.fragments.home.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/chatDetailsFragment"
        android:name="com.prof.reda.android.project.fooddelivery.views.fragments.home.ChatDetailsFragment"
        android:label="ChatDetailsFragment"
        tools:layout="@layout/fragment_chat_details">
        <action
            android:id="@+id/action_chatDetailsFragment_to_finishOrderFragment"
            app:destination="@id/finishOrderFragment" />
    </fragment>
    <fragment
        android:id="@+id/popularRestroFragment"
        android:name="com.prof.reda.android.project.fooddelivery.views.fragments.home.PopularRestaurantFragment"
        android:label="PopularRestroFragment"
        tools:layout="@layout/fragment_popular_restaurant"/>

    <fragment
        android:id="@+id/popularMenuFragment"
        android:name="com.prof.reda.android.project.fooddelivery.views.fragments.home.PopularMenuFragment"
        android:label="PopularMenuFragment"
        tools:layout="@layout/fragment_popular_menu"/>
    <fragment
        android:id="@+id/filterFragment"
        android:name="com.prof.reda.android.project.fooddelivery.views.fragments.home.FilterFragment"
        android:label="FilterFragment"
        tools:layout="@layout/fragment_filter"/>
    <fragment
        android:id="@+id/finishOrderFragment"
        android:name="com.prof.reda.android.project.fooddelivery.views.fragments.home.FinishOrderFragment"
        android:label="FinishOrderFragment"
        tools:layout="@layout/fragment_finish_order">
        <action
            android:id="@+id/action_finishOrderFragment_to_rateFoodFragment"
            app:destination="@id/rateFoodFragment" />
    </fragment>
    <fragment
        android:id="@+id/rateFoodFragment"
        android:name="com.prof.reda.android.project.fooddelivery.views.fragments.home.RateFoodFragment"
        android:label="RateFoodFragment"
        tools:layout="@layout/fragment_rate_food"/>
</navigation>